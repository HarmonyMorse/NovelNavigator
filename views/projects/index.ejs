<%- include('../partials/header') %>
<table id="projects-list">
    <thead>
        <tr>
            <th>Title</th>
            <th>Last Edited</th>
            <th>Characters</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        <% projects.forEach(function(p) { %>
            <tr>
                <td><%= p.title %></td>
                <td><%= p.updatedAt.getMonth() + 1 %>/<%= p.updatedAt.getDate() %>/<%= p.updatedAt.getFullYear() %></td>
                <td><%= p.characterCount %></td>
                <td><a href="/projects/<%= p._id %>">MORE</a></td>
            </tr>
        <% }); %>
    </tbody>
</table>
<%- include('../partials/footer') %>